<%= form_for [wiki, pic], html: { multipart: true } do |f| %>
   <div class="form-group">
     <%= f.label :caption, class: 'sr-only' %>
     <%= f.text_field :caption, class: 'form-control', placeholder: "Add A Caption" %>
   </div>
   <div class="form-group">
     <%= f.label :image %>
     <%= f.file_field :image, multiple: true %>
   </div>
   <%= f.submit "Submit", class: 'btn btn-default pull-right' %>
 <% end %>
