<div class="pic-show">
    <% if @wiki.pics.empty? %>
   <p> No Images Posted<p>
     <% else %>
  <% @wiki.pics.each do |pic| %>
  <div class="row col-md-4">
  <%=link_to (image_tag (pic.image.url(:album))), pic.image.url(:album), data: {lightbox: "eaxmple-set"}%>
  </div>
  <% end %>
  <% end %>
</div>
